#include <stdlib.h>
#include <stdio.h>

int factorielle(int n) {
  int i,fact;
  i = 0;
  fact = 1;
  //invariant de boucle fact = i!
  while (i!=n) {
    //{fact=i!} mais on doit faire apparaître i+1 donc fact*(i+1)=i!*(i+1)=(i+1)!
    i=i+1;
    //{fact*i = i!}
    fact = fact*i;
    //{fact = i!}
  //{fact=i! et i = n} conséquent
  }
  return fact;
}

int main(void){
  int n;
  scanf("\n%d",&n);
  printf("%d\n",factorielle(n));
}
