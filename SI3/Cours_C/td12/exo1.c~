#include <stdlib.h>
#include <stdio.h>

typedef enum{ carre , rond , triangle } forme;

void afficherForme(forme f) {
  switch(f) {
    case carre : printf("carr√©"); break;
    case rond : printf("rond"); break;
    case triangle : printf("triangle"); break;
    default : printf("forme  inconnue");
  }
}
