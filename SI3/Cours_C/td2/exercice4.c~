#include <stdlib.h>
#include <stdio.h>

int main(void) {
  enum booleen {Faux, Vrai};
  enum booleen a;
  enum booleen b;
  //type : en c on peut faire des types mais restrictions : idetificateur de constante enti√®re//
  a = Vrai;
  b = Faux;
  printf("a = %d\nb = %d", a,b)
  return EXIT_SUCCESS;
}
