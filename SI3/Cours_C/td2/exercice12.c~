#include <stdlib.h>
#include <stdio.h>
#include <math.h>
#include <assert.h>

int main(void) {
  float temps;
  int min, heure, seconde, centieme;
  scanf("%lf", &temps);
  assert(temps>=0);
  seconde = floor(temps);
  centieme = temps - seconde;
  printf("%d\n%d\n",seconde,centieme);
  return EXIT_SUCCESS;
}
