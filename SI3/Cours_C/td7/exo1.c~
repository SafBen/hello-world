#include <stdlib.h>
#include <stdio.h>

/* antécédent : n>0
 * conséquent : renvoie la somme 1 + 1/2 + 1/3 +...+ 1/n 
 */

double somme(int n) {
  sum = 0
  for (int i = 1; i <= n; i++)
    sum += 1/i;
}

int main(){
  int n;
  scanf("%d\n",&n);
  printf("somme = %d\n",somme(n));
  return EXIT_SUCCESS;
}
