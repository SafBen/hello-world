#include

typedef struct {
  double reel;
  double img;
} Complexe;

void ecrireComplexe(Complexe c){
  printf("(%f,%f)\n",c.reel,c.img);
}

Complexe initComplexe(double r, double i){
  Complexe c;
  c.reel = r;
  c.img = i;
  return c;
}

int main(){
  Complexe c;
  double r;
  double i;
  c = initComplexe(r,i);
  ecrireComplexe(c);
  return 1;
}
