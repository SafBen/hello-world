#include <stdlib.h>
#include <stdio.h>
#include <ctype.h>

int main(void){
  int n;
  int c;
  scanf("%d\n",&n);//lire entrée standard
  while ( (c=getchar()) != EOF ){
    //c n'est pas la fin du fichier
    if (islower(c)){
      //c est une minuscule
      c+=n;
      if (c>"z")
	c-=26;
    }
    if (isupper(c)){
      //c est une majuscule
      c+=n;
      if (c>"Z")
	c-=26;
    }
    putchar(c); // on écrit sur l'entrée standard
  }
  return EXIT_SUCCESS;
}
