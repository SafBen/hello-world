#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>

int main(void){
  int c;
  int nbLettre = 0, nbChiffre = 0, nbEspace = 0;
  while ((c=getchar()) != EOF){
    while (isalpha(c))
    nbLettre ++;
    while (isdigit(c))
    nbChiffre ++;
    while (isspace(c))
    nbEspace ++;
  }
  printf("%d %d %d",nbLettre,nbChiffre,nbEspace);
}
