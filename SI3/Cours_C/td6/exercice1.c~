#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>

int main(void){
  //L'entrée standard contient une suite de chiffres
  int c = getchar(); // on lit le premier caractère
  int n = 0;
  do { // c est un chiffre
    n = n*10 + c-'0';
  }
  while (isdigit(c=getchar())) //isdigit test si le paramètre est un chiffre
    //le caractère courant est un chiffre
  // n est égal au nombre lu sur l'entrée standard
  printf("%d\n",n*n);
  return EXIT_SUCCESS;
}
